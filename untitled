for line in reader:
		del line[0:4]
        runindex = label_index+4096
        label = line.pop(runindex)
        if label == "U":
                labelnum = "0"
        elif label == "B":
                labelnum = "1"
        elif label == "A":
                continue
        del line[4096:]
        new_line = construct_line( labelnum, line )
        print len(new_line)
        if len(new_line) == 2:
                continue
        print new_line
        o.write( new_line )

        

